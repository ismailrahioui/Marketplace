{
    "version": 2,
    "builds": [
      {
        "src": "marketplace/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static",
        "config": {
          "distDir": "static"
        }
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/"
      },
      {
        "src": "/",
        "dest": "marketplace/wsgi.py"
      }
    ]
  }
  